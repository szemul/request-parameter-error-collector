{
  "name": "szemul/request-parameter-error-collector",
  "description": "Request parameter error collector interface with related enums",
  "license": "MIT",
  "minimum-stability": "stable",
  "require": {
    "php": "^8.1"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0.0",
    "jetbrains/phpstorm-attributes": "^1.0",
    "mockery/mockery": "^1.0",
    "phpstan/phpstan": "^1.4.0",
    "phpunit/phpunit": "^9.5.5"
  },
  "autoload": {
    "psr-4": {
      "Szemul\\RequestParameterErrorCollector\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Szemul\\RequestParameterErrorCollector\\Test\\": "tests"
    }
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true,
    "allow-plugins": false
  }
}
